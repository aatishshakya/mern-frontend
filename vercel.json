{
  "version": 2,
  "builds": [
    {
      "src": "src/**/*",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "http://localhost:5000/api/users"
    }
  ],
  "env": {
    "API_URL": "https://mern-backend-test-tau.vercel.app/api/users"
  }
}
